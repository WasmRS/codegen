{
  "name": "@nanobus/tinygo",
  "version": "0.0.1",
  "description": "TinyGo code generation for Iotas",
  "keywords": [
    "iota",
    "nanobus",
    "apex",
    "codegen",
    "webassembly",
    "wasm",
    "tinygo",
    "go"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "templates",
    "dist",
    "README.md",
    "test",
    "src",
    "docs",
    "definitions",
    "templates",
    "*.js",
    "*.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "devbuild": "npm run format:fix && npm run build && apex install file:.",
    "prebuild": "npm run clean",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "shx rm -rf dist",
    "test": "echo todo",
    "style": "npm run format -- --list-different && npm run lint",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "format": "prettier --config ../../config/.prettierrc \"src/**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}\"",
    "format:fix": "npm run format -- --write",
    "lint": "eslint -c ../config/.eslintrc.json src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prerelease": "npm run build"
  },
  "dependencies": {
    "@apexlang/core": "^0.1.1",
    "@apexlang/codegen": "^0.0.16"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1",
    "shx": "0.3.4",
    "typescript": "^4.8.4"
  }
}
